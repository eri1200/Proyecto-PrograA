@model IEnumerable<ProyectoMvc.Models.TipoIdentificacion>

@{ ViewData["Title"] = "Listado de tipos de identificación"; }

<h1 class="text-center"><i class="fas fa-comments-dollar text-success"></i> Listado de Tipos de Identificacion</h1>

<table class="table" id="tblTiposIdentificacion">
    <thead class="thead-light">
        <tr>
            <th>Descripción</th>
            <th>
                <a onclick="@Url.Action("Upsert", "TipoIdentificacion", null, Context.Request.Scheme)" class="btn btn-success text-white"><i class="fas fa-random"></i> Nuevo</a>
            </th>
        </tr>
    </thead>
</table>

@section Scripts
{
    <partial name="_ValidationScriptsPartial" />
    <script type="text/javascript" src="~/js/TipoIdentificacion.js"></script>
}